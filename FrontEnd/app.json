{
  "expo": {
    "name": "EcoSphere",
    "slug": "EcoSphere",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/ecosphere.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.marina828282.EcoSphere",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/ecosphere.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.marina828282.EcoSphere"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/ecosphere.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/ecosphere.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "74878be7-e32e-449e-b5a0-11247ec9b399"
      }
    }
  }
}
